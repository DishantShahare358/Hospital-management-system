{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "cleanUrls": true,
  "rewrites": [
    { "source": "/:path*", "destination": "/index.html", "missing": [{ "type": "file" }] }
  ]
}
